<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>HttpSessionProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>ServletOne</servlet-name>
  	<servlet-class>com.example.servlet.MyServlet1</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ServletTwo</servlet-name>
  	<servlet-class>com.example.servlet.MyServlet2</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ServletThree</servlet-name>
  	<servlet-class>com.example.servlet.MyServlet3</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletOne</servlet-name>
  	<url-pattern>/servlet1.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ServletTwo</servlet-name>
  	<url-pattern>/servlet2.do</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ServletThree</servlet-name>
  	<url-pattern>/servlet3.do</url-pattern>
  </servlet-mapping>
  
  <listener>
  	<listener-class>com.example.listener.SessionListener</listener-class>
  </listener>
  
</web-app>